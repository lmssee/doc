{
  "name": "earthnut-doc-of-docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc",
    "prepare": "husky",
    "eslint": "jja cls && eslint . ",
    "vjj": "vjj -n",
    "diff": "jja pkg --diff=淘宝",
    "prettier": "jja cls && prettier . --write",
    "push": "gvv",
    "push:d": "gvv kind=deploy",
    "docusaurus": "docusaurus",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "start": "jja cls run PORT=9463 NODE_OPTIONS='--max-old-space-size=768' docusaurus start",
    "reinstall": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "dev": "pnpm start",
    "build:local": "chmod +x ./scripts/build.sh && ./scripts/build.sh com io",
    "build:only": "chmod +x ./scripts/build-zip.sh && ./scripts/build-zip.sh",
    "deploy:cloudflare": "jja cls run TARGET_DOMAIN=io pnpm docusaurus build && wrangler pages deploy build",
    "serve": "pnpm run build:only && docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "beautify": "pnpm prettier",
    "write-heading-ids": "docusaurus write-heading-ids",
    "swizzle": "echo 'pnpm run swizzle @docusaurus/theme-classic  Footer --eject' && docusaurus swizzle "
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-content-docs": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-common": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "a-js-tools": "^1.0.12",
    "a-type-of-js": "^1.0.7",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "enr": "1.0.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "wrangler": "^4.44.0",
    "xcn": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@eslint/js": "^9.38.0",
    "@types/react": "^19.2.2",
    "babel-plugin-filter-imports": "^4.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "gvv": "^0.1.8",
    "husky": "^9.1.7",
    "jja": "^2.3.21",
    "lint-staged": "^16.2.6",
    "pjj": "^1.0.4",
    "prettier": "^3.6.2",
    "sass-loader": "^16.0.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vjj": "^1.0.11"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}