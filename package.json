{
  "name": "earthnut-doc-of-docusaurus",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": " PORT=9463 NODE_OPTIONS='--max-old-space-size=768' docusaurus start",
    "dev": "TARGET_DOMAIN=lmssee  npm start",
    "push:submit": "bash -c 'git add . && git commit -m \"submit:  $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "push:deploy": "bash -c 'git add . && git commit -m \"deploy:  $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "build:local": "chmod +x build.sh && ./build.sh",
    "build:only": "npx docusaurus clear && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "beautify": "npm run prettier",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prepare": "husky",
    "eslint": "npx ixxx cls && eslint src",
    "prettier": "npx ixxx cls && prettier . --write"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-content-docs": "^3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@mdx-js/react": "^3.1.0",
    "@types/sass": "^1.43.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "oops-ui": "^0.0.13",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.86.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@eslint/js": "^9.22.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-mdx": "^3.2.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sass-loader": "^16.0.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
