{
  "name": "earthnut-doc-of-docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": " PORT=9463 NODE_OPTIONS='--max-old-space-size=768' docusaurus start",
    "dev": "TARGET_DOMAIN=lmssee  npm start",
    "push": "gvv",
    "push:deploy": "gvv",
    "diff": "jja pkg --diff",
    "build:local": "chmod +x build.sh && ./build.sh",
    "build:only": "docusaurus clear && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "beautify": "npm run prettier",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prepare": "husky",
    "eslint": "jja cls && eslint . ",
    "vjj": "vjj -n",
    "prettier": "jja cls && prettier . --write"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@types/sass": "^1.45.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@eslint/js": "^9.31.0",
    "babel-plugin-filter-imports": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "gvv": "^0.1.2",
    "husky": "^9.1.7",
    "jja": "^2.3.11",
    "lint-staged": "^16.1.2",
    "pjj": "^1.0.2",
    "prettier": "^3.6.2",
    "sass-loader": "^16.0.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vjj": "^1.0.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
